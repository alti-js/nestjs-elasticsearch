    "@alti/logging": "<rootDir>/../libs/logging/src",
    "@alti/queue-manager/(.*)": "<rootDir>/../libs/queue-manager/src/$1",
    "@alti/queue-manager": "<rootDir>/../libs/queue-manager/src",
    "@alti/apple-auth/(.*)": "<rootDir>/../libs/apple-auth/src/$1",
    "@alti/apple-auth": "<rootDir>/../libs/apple-auth/src",
    "@alti/galileo-ft/(.*)": "<rootDir>/../libs/galileo-ft/src/$1",
